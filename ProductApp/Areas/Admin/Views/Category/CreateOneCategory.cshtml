@model ProductForInsertionDto

<div class="d-flex justify-content-end">
    <a asp-action="Index" class="btn btn-outline-danger">Category List</a>
</div>

<form method="post"
      asp-action="CreateOneCategory"
      asp-controller="Category">
    <div class="text-danger" asp-validation-summary="All"></div>

    <div class="form-group my-3">
        <input type="text"
               placeholder="Kategori İsmi!"
               class="form-control" />
    </div>
    <button class="btn btn-outline-primary" type="submit">Kategori Oluştur!</button>

</form>