@if (TempData["info"] is not null)
{
    <div class="alert alert-secondary" role="alert">
        @TempData["info"]
    </div>
}

@if (TempData["infoCategory"] is not null)
{
    <div class="alert alert-warning" role="alert">
        @TempData["infoCategory"]
    </div>
}
@if (TempData["infoProduct"] is not null)
{
    <div class="alert alert-warning" role="alert">
        @TempData["infoProduct"]
    </div>
}